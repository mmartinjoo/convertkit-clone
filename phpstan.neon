includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:
    paths:
        - app
        - src

    level: 1
    excludePaths:
        - 'app/Http/Middleware'
        - 'app/Http/Kernel.php'
        - 'app/Providers'
        - 'app/Console/Kernel.php'
        - 'app/Exceptions/Handler.php'
    checkMissingIterableValueType: false
    noUnnecessaryCollectionCallExcept: ['pluck']
